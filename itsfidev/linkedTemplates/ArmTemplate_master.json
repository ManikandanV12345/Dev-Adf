{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "itsfidev"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"AzureSqlDatabase3_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase3'"
		},
		"testing_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'testing'"
		},
		"AzureKeyVault2_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://sqlkyvaults.vault.azure.net/"
		},
		"AzureSqlDatabase2_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "@linkedService().SecretName"
		},
		"DynamicKV_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "@{linkedService().VaultName}"
		},
		"itsfi_blob_ac_key01_properties_typeProperties_sasUri_secretName": {
			"type": "string",
			"defaultValue": "blob"
		},
		"itsfi_dfs_ac_key01_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://itsfiadls.dfs.core.windows.net"
		},
		"itsfi_sqlmi_dataframework_key01_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "sqlsecret"
		},
		"trigger2_properties_ListeningCustomevents_parameters_param1": {
			"type": "string",
			"defaultValue": "@triggerBody().event.data._keyName_param1"
		},
		"trigger2_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/7f43217e-5dd6-4b9b-8ba6-149b75296777/resourceGroups/itsfirsg/providers/Microsoft.EventGrid/topics/Mani"
		},
		"trigger3_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/7f43217e-5dd6-4b9b-8ba6-149b75296777/resourceGroups/itsfirsg/providers/Microsoft.EventGrid/topics/Mani"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "itsfidev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"AzureSqlDatabase3_connectionString": {
						"value": "[parameters('AzureSqlDatabase3_connectionString')]"
					},
					"testing_connectionString": {
						"value": "[parameters('testing_connectionString')]"
					},
					"AzureKeyVault2_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault2_properties_typeProperties_baseUrl')]"
					},
					"DynamicKV_properties_typeProperties_baseUrl": {
						"value": "[parameters('DynamicKV_properties_typeProperties_baseUrl')]"
					},
					"AzureSqlDatabase2_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('AzureSqlDatabase2_properties_typeProperties_connectionString_secretName')]"
					},
					"itsfi_blob_ac_key01_properties_typeProperties_sasUri_secretName": {
						"value": "[parameters('itsfi_blob_ac_key01_properties_typeProperties_sasUri_secretName')]"
					},
					"itsfi_dfs_ac_key01_properties_typeProperties_url": {
						"value": "[parameters('itsfi_dfs_ac_key01_properties_typeProperties_url')]"
					},
					"itsfi_sqlmi_dataframework_key01_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('itsfi_sqlmi_dataframework_key01_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "itsfidev_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"trigger2_properties_ListeningCustomevents_parameters_param1": {
						"value": "[parameters('trigger2_properties_ListeningCustomevents_parameters_param1')]"
					},
					"trigger2_properties_typeProperties_scope": {
						"value": "[parameters('trigger2_properties_typeProperties_scope')]"
					},
					"trigger3_properties_typeProperties_scope": {
						"value": "[parameters('trigger3_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'itsfidev_ArmTemplate_0')]"
			]
		}
	]
}